<% include partials/headerlayout %>
<!DOCTYPE html>
<html lang="en">
<head>

 <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
  <script type="text/javascript" src="//code.jquery.com/jquery-2.1.4.min.js"></script>
  <script type="text/javascript" src="/js/markerclusterer.js"></script>
  

  <link rel="stylesheet" href="/css/style.css">

	<meta charset="UTF-8">
	<title>San Francisco</title>

	 <style>
      #map-canvas {
        width: 800px;
        height: 600px;
        background-color: #CCC;
        float:left;
      }
    </style>
</head>
<body>
<div class="panel panel-default">
  <div class="panel-body">
	<div id="map-canvas"></div>
  <div style = "text-align:center" id = "x">

<a class="btn btn-default" href = "/crimes">View list of crimes </a>
<a class="btn btn-default" style = "display: inline-block;" id = "data">Get crimes nearby</a>

<table style = "margin-top: 15px" >
<% crimes.forEach(function(crime){ %>
  <tr style = "text-align:left">
    <td><img style="width:50px;padding:3px" src = "<%= crime.image%>"></td>
    <td><a href="/crimes/<%= crime._id%>/"><%= crime.name%></a></td>    
    <td><a href="/crimes/<%= crime.id%>/edit">Edit</a><a id = "pan"> Focus</a></td>
  </tr>
<% })%>
</table>
</div>




