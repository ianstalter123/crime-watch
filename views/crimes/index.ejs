<head>
  <meta charset="UTF-8">
  <title>Document</title>

   <style>
  #main { 
    width: 800px;
    margin: 0 auto;
}
#sidebar    {
    width: 300px;
    height: 400px;
    
    float: left;
}

#page-wrap  {
    width: 400px;
  
    height: 400px;
    margin-left: 300px;
}

    </style>


  <script type="text/javascript" src="//code.jquery.com/jquery-2.1.4.min.js"></script>

  <script type="text/javascript" src="/js/chart.js"></script>
   <script type="text/javascript" src="/js/main.js"></script>

   <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">

</head>

<body>




<h1>SF Crime</h1>

<div class="nav">
      <ul>
        <li class="home"><a href="/search">Search for crimes</a></li>
        <li class="tutorials"><a class="active" href="/">Goto crime map</a></li>
        <li class="tutorials"><a class="active" href="/logout">Logout</a></li>
   
      </ul>
    </div>


<div id="main">
  <div id="sidebar">
<div id="canvas-holder">
      <canvas id="canvas" height="225" width="300"></canvas>
    </div>


    </div>
  <div id="page-wrap">

<table class = "table">
<tr>
<td>resolved crimes: </td>
<td ><a id = 'resolved'></a></td>
<td>uresolved crimes: </td>
<td ><a id = 'unresolved'></a></td>
</tr>
<% crimes.forEach(function(crime){ %>
  <tr>
    <td><img style="width:50px;" src = "<%= crime.image%>"></td>
    <td><a href="/crimes/<%= crime._id%>/"><%= crime.name%></a></td>    
    <td><a href="/crimes/<%= crime.id%>/edit">Edit</a></td>
  </tr>
<% })%>
</table>
</div>
</div>

 
 

 
  </body>
  

